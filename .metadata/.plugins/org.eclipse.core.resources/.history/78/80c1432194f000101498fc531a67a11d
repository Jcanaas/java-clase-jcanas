	package ejercicios;
	
	import java.util.*;
	
	public class MÃ ximdeMatriu {
	    public static void main(String[] args) {
	        Scanner sc = new Scanner(System.in);
	        int n = sc.nextInt();
	        for(int i = 0; i < n; i++) {
	        	int x = Integer.parseInt(sc.next());
	        	int y = Integer.parseInt(sc.next());;
	        	System.out.println(y);
	        	int[][]  matriu = new int[x][y];
	        	for(int w = 0; w < x; w++) {
	        		for(int z = 0; z < y; z++) {
		        		matriu[w][z] = sc.nextInt(); 
		        	}
	        	}
	        	int[]  ma = new int[2];
	        	int mac = 0;
	        	for(int w = 0; w < x; w++) {
	        		for(int z = 0; z < y; z++) {
		        		if(matriu[w][z] > mac) {
		        			ma[0] = x;
		        			ma[1] = y;
		        		}
		        	}
	        	}
	        	
	        	System.out.println(ma[0]);
	        	System.out.println(ma[1]);
	        }
	        
	        
	}
	}
 