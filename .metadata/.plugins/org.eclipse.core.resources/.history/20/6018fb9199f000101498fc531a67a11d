	package ejercicios;
	
	import java.util.*;
	
	public class alfil {
	    public static void main(String[] args) {
	        Scanner sc = new Scanner(System.in);
	        int n = sc.nextInt();
	        int[][]  matriu = new int[8][8];
	        sc.nextLine();
	        for (int i = 0; i < n; i++) {
	        String s = sc.nextLine();
	        int f0 = s.charAt(1) - '0';	        
	        int c0 = con(s.charAt(0));
	        
	        
	        int suma = 0;
	        while(f <= 7 && c <= 7 && f >= 0 && c >= 0) {
	        	suma++;
	        	f++;
	        	c++;
	        }
	        
	        
	        
	        System.out.println(suma);	

	        while(f <= 7 && c <= 7 && f >= 0 && c >= 0) {
	        	suma++;
	        	f++;
	        	c--;
	        }
	        
	        
	        
	        
	        System.out.println(suma);	

	        while(f <= 7 && c <= 7 && f >= 0 && c >= 0) {
	        	suma++;
	        	f--;
	        	c--;
	        }
	        
	       
	       
	        System.out.println(suma);	

	        while(f <= 7 && c <= 7 && f >= 0 && c >= 0) {
	        	suma++;
	        	f--;
	        	c++;
	        }
	        
	        
	        	
	        System.out.println(suma);	
	        }	        
	    }
	    static int con(char c) {
	        if(c == 'a') return 0;
	        else if(c == 'b') return 1;
	        else if(c == 'c') return 2;
	        else if(c == 'd') return 3;
	        else if(c == 'e') return 4;
	        else if(c == 'f') return 5;
	        else if(c == 'g') return 6;
	        else if(c == 'h') return 7;
	        return 0;
	    }
	}
