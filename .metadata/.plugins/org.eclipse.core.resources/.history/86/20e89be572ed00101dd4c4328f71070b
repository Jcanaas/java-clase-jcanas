	package ejercicios;
	
	import java.util.*;
	
	public class alfil {
	    public static void main(String[] args) {
	        Scanner sc = new Scanner(System.in);
	        int n = sc.nextInt();
	        int[][]  matriu = new int[8][8];
	        sc.nextLine();
	        for (int i = 0; i < n; i++) {
	        String s = sc.nextLine();
	        int f = s.charAt(1);	
	        int c = con(s.charAt(0));
	        int suma = 0 ;
	        while(f > 8 || c > 8) {
	        	suma++;
	        	f++;
	        	c++;
	        }
	        while(f > 8 || c > 8) {
	        	suma++;
	        	f++;
	        	c++;
	        }
	        	
	        	
	        }	        
	    }
	    static int con(char c) {
	    	int r = 0;
	    	if(c == 'a') {r = 0; }
	    	else if(c == 'b') {r = 1; }
	    	else if(c == 'c') {r = 2; }
	    	else if(c == 'd') {r = 3; }
	    	else if(c == 'e') {r = 4; }
	    	else if(c == 'g') {r = 5; }
	    	else if(c == 'h') {r = 6; }
	    	else if(c == 'i') {r = 7; }
	    	return r;
	    }
	}
